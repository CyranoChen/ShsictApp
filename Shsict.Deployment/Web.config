  <connectionStrings>
    <!-- SqlServer-->
    <add connectionString="server=10.10.4.133;database=shsict;user id=sa;pwd=sa" name="Shsict.MsSql.ConnectionString"/>
    <!-- Oracle VIEW -->
    <add connectionString="User ID=sdcwp;password=sdcwp;data source=SDAPP;Pooling=true;Min Pool Size=10;" name="Shsict.Oracle.View.ConnectionString"/>
    <!-- Oracle Table -->
