    <!-- 卡车刷新频率（整数） 单位：分钟 -->
    <add key="TruckRefreshRate" value="15"/>

    <!-- 公告栏刷新频率（整数） 单位：分钟 -->
    <add key="NoticeRefreshRate" value="5"/>

    <!-- 收藏夹刷新频率（整数） 单位：分钟 -->
    <add key="FavouriteRefreshRate" value="5"/>

    <!-- 箱子刷新频率(整数) 单位:分钟 -->
